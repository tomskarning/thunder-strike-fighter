<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <corners android:radius="10dp" />
            <gradient android:angle="270" android:endColor="#AC78B1" android:startColor="#AC78B1" />
            <stroke android:width="1dp" android:color="#FF56E9" />
        </shape>  
    </item>
    <item>
        <shape>
            <corners android:radius="10dp" />
            <gradient android:angle="270" android:endColor="#831C7F" android:startColor="#D22CCD" />
            <stroke android:width="1dp" android:color="#FF56E9" />
        </shape>
	</item>
</selector>